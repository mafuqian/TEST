<?xml version="1.0"?>
<Document type="update" table="MBT_230_EX_RPT"
	sql="select * from MBT_230_EX_RPT where 1=1 and  DATA_STATUS='24' #{RPT_DATE,date=yyyyMMdd,and}  #{ORG_ID,string,and,IN} order by DATA_ID"
	code="232" desc="个人担保账户按段更正请求记录">
	<Element type="InfoBase" name="BsSgmt" data="Entity"
		table="MBT_230_EX_RPT">
		<Element type="Colum" name="InfRecType" field="B_INF_REC_TYPE" />
		<Element type="Colum" name="ModRecCode" field="MOD_REC_CODE" />
		<Element type="Colum" name="RptDate" format="date={yyyy-MM-dd}" field="RPT_DATE" />
		<Element type="Colum" name="AcctType" field="B_ACCT_TYPE" />
		<Element type="Colum" name="MdfcSgmtCode" field="PART_TYPE" />
	</Element>

	<Element type="InfoUpdate" name="MdfcSgmt">
		<Element type="InfoBase" name="GuarAcctBsSgmt" data="Entity"
			table="MBT_230_EX_RPT"  script="PART_TYPE=='B'">
			<Element type="Colum" name="InfRecType"
				field="B_INF_REC_TYPE" />
			<Element type="Colum" name="AcctType" field="B_ACCT_TYPE" />
			<Element type="Colum" name="AcctCode" field="B_ACC_CODE" />
			<Element type="Colum" name="RptDate" format="date={yyyy-MM-dd}"  field="B_RPT_DATE" />
			<Element type="Colum" name="RptDateCode"
				field="B_RPT_DATE_CODE" />
			<Element type="Colum" name="Name" field="B_NAME" />
			<Element type="Colum" name="IDType" field="B_ID_TYPE" />
			<Element type="Colum" name="IDNum" field="B_ID_NUM" />
			<Element type="Colum" name="MngmtOrgCode"
				field="B_MNGMT_ORG_CODE" />
		</Element>

		<Element type="InfoBase" name="GuarAcctBsInfSgmt"
			data="Entity" table="MBT_230_EX_RPT" script="PART_TYPE=='C'">
			<Element type="Colum" name="BusiLines" field="C_BUSI_LINES" />
			<Element type="Colum" name="BusiDtilLines"
				field="C_BUSIDTIL_LINES" />
			<Element type="Colum" name="OpenDate"  format="date={yyyy-MM-dd}" field="C_OPEN_DATE" />
			<Element type="Colum" name="AcctCredLine"
				field="C_ACCT_CRED_LINE" />
			<Element type="Colum" name="Cy" field="C_CY" />
			<Element type="Colum" name="DueDate" format="date={yyyy-MM-dd}" field="C_DUE_DATE" />
			<Element type="Colum" name="GuarMode" field="C_GUAR_MODE" />
			<Element type="Colum" name="OthRepyGuarWay"
				field="C_OTH_REPY_GUAR_WAY" />
			<Element type="Colum" name="SecDep" field="C_SEC_DEP" />
			<Element type="Colum" name="CtrctTxtCd"
				field="C_CTRCT_TXT_CD" />
		</Element>

		<Element type="InfoBase" name="GuarRltRepymtInfSgmt"
			data="Entity" table="MBT_230_EX_RPT" script="PART_TYPE=='D'">
			<Element type="Colum" name="AcctStatus" field="D_ACCT_STATUS" />
			<Element type="Colum" name="LoanAmt" field="D_LOAN_AMT" />
			<Element type="Colum" name="RepayPrd" format="date={yyyy-MM-dd}"
				field="D_REPAY_PRD_DATE" />
			<Element type="Colum" name="FiveCate" field="D_FIVE_CATE" />
			<Element type="Colum" name="FiveCateAdjDate" format="date={yyyy-MM-dd}"
				field="D_FIVE_CATE_ADJ_DATE" />
			<Element type="Colum" name="RiEx" field="D_RI_EX" />
			<Element type="Colum" name="CompAdvFlag"
				field="D_COMP_ADV_FLAG" />
			<Element type="Colum" name="CloseDate"  format="date={yyyy-MM-dd}" field="D_CLOSE_DATE" default=""/>
		</Element>

		<Element type="InfoBase" name="RltRepymtInfSgmt"
			data="Collection" size="RltRepymtNm" entity="RltRepymtInf"
			table="MBT_230_E_RPT"
			sql="select ${colums} from  MBT_230_E_RPT where MBT_230_E_RPT.PDATA_ID=${DATA_ID}" script="PART_TYPE=='E'">
			<Element type="Colum" name="InfoIDType" field="INFO_ID_TYPE" />
			<Element type="Colum" name="ArlpName" field="ARLP_NAME" />
			<Element type="Colum" name="ArlpCertType"
				field="ARLP_CERT_TYPE" />
			<Element type="Colum" name="ArlpCertNum"
				field="ARLP_CERT_NUM" />
			<Element type="Colum" name="ArlpType" field="ARLP_TYPE" />
			<Element type="Colum" name="ArlpAmt" field="ARLP_AMT" />
			<Element type="Colum" name="WartySign" field="WARTY_SIGN" />
			<Element type="Colum" name="MaxGuarMcc" field="MAX_GUAR_MCC" />
		</Element>

		<Element type="InfoBase" name="GuarMotgaCltalCtrctInfSgmt"
			data="Collection" size="CcNm" entity="CccInf" table="MBT_230_F_RPT"
			sql="select ${colums} from  MBT_230_F_RPT where MBT_230_F_RPT.PDATA_ID=${DATA_ID}" script="PART_TYPE=='F'">
			<Element type="Colum" name="Ccc" field="CCC" />
		</Element>
	</Element>


</Document>