        <Element type="Colum" name="LatAgrrRpyAmt" field="H_LAT_AGRR_RPY_AMT" script="B_ACCT_TYPE !='C1'"/>
        <Element type="Colum" format="date={yyyy-MM-dd}" name="NxtAgrrRpyDate" field="H_NXT_AGRR_RPY_DATE" default="" script="B_ACCT_TYPE !='C1'"/>
        <Element type="Colum" format="date={yyyy-MM-dd}" name="CloseDate" field="H_CLOSE_DATE"  default=""/>
    </Element>

    <Element type="InfoBase" name="AcctSpecTrstDspnSgmt" data="Collection" size="CagOfTrdNm" entity="CagOfTrdInf" table="MBT_410_I_RPT" sql="select ${colums} from  MBT_410_I_RPT where MBT_410_I_RPT.PDATA_ID=${DATA_ID}">
        <Element type="Colum" name="ChanTranType" field="I_CHAN_TRAN_TYPE"/>
