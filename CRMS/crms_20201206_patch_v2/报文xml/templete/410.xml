        <Element type="Colum" name="OthRepyGuarWay" field="C_OTH_REPY_GUAR_WAY" script="B_ACCT_TYPE !='C1'"/>
        <Element type="Colum" name="LoanTimeLimCat" field="C_LOAN_TIME_LIM_CAT" script="B_ACCT_TYPE=='D1'||B_ACCT_TYPE=='R4'"/>
        <Element type="Colum" name="LoaFrm" field="C_LOA_FRM" script="B_ACCT_TYPE !='C1'"/>
        <Element type="Colum" name="ActInvest" field="C_ACT_INVEST" script="B_ACCT_TYPE=='D1'||B_ACCT_TYPE=='R1'||B_ACCT_TYPE=='R4'"/>
        <Element type="Colum" name="FundSou" field="C_FUND_SOU" script="B_ACCT_TYPE !='C1'"/>
        <Element type="Colum" name="AssetTrandFlag" field="C_ASSET_TRAND_FLAG" script="B_ACCT_TYPE=='D1'||B_ACCT_TYPE=='R1'||B_ACCT_TYPE=='R4'"/>
    </Element>


