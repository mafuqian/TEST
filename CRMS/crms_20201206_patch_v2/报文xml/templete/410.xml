<Document type="Info" name="EnAcctInf" table="MBT_410_RPT" sql="select * from MBT_410_RPT where 1=1 and  DATA_STATUS='24' #{RPT_DATE,date=yyyyMMdd,and }  #{ORG_ID,string,and,IN} order by DATA_ID" code="410" desc="企业借贷账户信息记录">
