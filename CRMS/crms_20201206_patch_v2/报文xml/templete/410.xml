        <Element type="Colum" name="TotOverd" field="H_TOT_OVERD" script="B_ACCT_TYPE !='C1'"/>
        <Element type="Colum" name="OverdPrinc" field="H_OVERD_PRINC" script="B_ACCT_TYPE !='C1'"/>
        <Element type="Colum" name="OverdDy" field="H_OVERD_DY" script="B_ACCT_TYPE !='C1'"/>
