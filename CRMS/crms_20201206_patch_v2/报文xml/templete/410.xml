        <Element type="Colum" name="RepayMode" field="C_REPAY_MODE" script="B_ACCT_TYPE=='D1'||B_ACCT_TYPE=='R1'||B_ACCT_TYPE=='R4'"/>
        <Element type="Colum" name="RepayFreqcy" field="C_REPAY_FREQCY" script="B_ACCT_TYPE=='D1'||B_ACCT_TYPE=='R1'||B_ACCT_TYPE=='R4'"/>
