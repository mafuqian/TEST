<?xml version="1.0"?>
<Document type="Info" name="BsSgmt"
          table="MBT_210_EX_RPT" sql="select * from MBT_210_EX_RPT where 1=1 and  DATA_STATUS='24' #{RPT_DATE,date=yyyyMMdd,and }  #{ORG_ID,string,and,IN}  order by DATA_ID" code="212" desc="个人借贷账户按段更正请求记录">
